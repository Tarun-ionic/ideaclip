    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 14 Pro Max\"",
    "start": "npx react-native  start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build-apk": "cd android && ./gradlew clean && npm run bundle && ./gradlew assembleRelease",
    "build-apkw": "cd android && gradlew clean && npm run bundle && gradlew assembleRelease",
    "cleanw": "cd android && gradlew clean && cd ..",
    "build-bundle": " cd android  && ./gradlew clean && npm run bundle && ./gradlew bundleRelease",
    "build-bundlew": " cd android  && gradlew clean && npm run bundle && gradlew bundleRelease",
    "copy": "copyfiles -u 4 src/assets/video/img/*  node_modules/react-native-video-player/assets/img",
    "copy-tab-viewer": "copyfiles -u 2 node_modules/react-native-scrollable-tab-view/*  node_modules/react-native-emoji-board/node_modules/react-native-scrollable-tab-view",
